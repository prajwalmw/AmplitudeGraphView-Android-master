<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="suman.brose.voicerecorder">
    <uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.suman.voice.voicerecorder.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.suman.voice.voicerecorder.MainActivity1">

        </activity>
        <activity android:name="com.suman.voice.voicerecorder.Calibration">
        </activity>
        <activity android:name="com.suman.voice.voicerecorder.Plot">
        </activity>
        <activity android:name="com.suman.voice.voicerecorder.RespiratoryRate">
        </activity>
    </application>

</manifest>
